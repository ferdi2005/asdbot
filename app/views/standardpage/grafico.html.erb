<% unless @sender.nil? %>
  <%= line_chart Asd.where(sender: @sender).group_by_day(:created_at).count %>
<% end %> 

<% unless @group.nil? %>
    <%= line_chart Asd.where(group: @group).group_by_day(:created_at).count %>
<% end %>

<script>
if (getAllUrlParams().username !== undefined) {
  Swal.fire({
  position: 'top-end',
  type: 'success',
  title: 'Ecco qui il grafico per ' + getAllUrlParams().username,
  showConfirmButton: false,
  timer: 1500
})
}
</script>
